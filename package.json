{
  "name": "mfe-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run preview",
    "dev": "npm run build -- --mode dev && npm run serve -- --mode dev",
    "qa": "npm run build -- --mode qa && npm run serve -- --mode qa",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "test": "jest --watchAll",
    "test:cov": "jest --coverage .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview ",
    "serve": "vite ",
    "prettier": "prettier 'src/**/**/*.?(js)' --write",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint 'src/**/*.css' --cache --fix || true"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.10.1",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "jwt-decode": "^4.0.0",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@originjs/vite-plugin-federation": "^1.3.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/ramda": "^0.29.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/redux-mock-store": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^29.5.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-sonar": "^0.2.16",
    "jest-sonar-reporter": "^2.0.0",
    "postcss": "^8.4.31",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-rem": "^2.0.4",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
